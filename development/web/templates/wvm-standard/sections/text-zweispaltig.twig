{% set data = section.data %}

<section class="section">
    <div class="handle">{{data.general_bezeichnung}}</div>
    <div class="inner wow fadeInUp" style="visibility: visible; animation-name: fadeInUp;">
        <h3 class="title">{{data.general_ueberschrift}}</h3>
        <div class="bodytext columns columns--pull">
            {{data.general_text|raw}}
        </div>
    </div>
</section>