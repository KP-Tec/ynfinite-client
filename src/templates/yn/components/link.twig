<a
    href="{{ link.href }}"
    {%- if link.css_id %}
        id="{{link.css_id}}"
    {% endif -%}
    {%- if classes or link.css_class %}
        class="{{ classes }} {{link.css_class}}"
    {% endif -%}
	{%- if link.title %}
        title="{{ link.title }}"
    {% endif -%}
    {%- if link.blank %}
        target="_blank"
    {% endif -%}

    {%- if link.data_attribute %}
        {{link.data_attribute}}
    {% endif -%}
>
	{{link.text}}
</a>
