<script type="application/ld+json">
	{
		"@context": "https://schema.org",
		"@type": "{{_yn_schema.variant|raw}}",
		{% if _yn_schema.name %}
			"name": "{{_yn_schema.name|raw}}",
		{% endif %}
		{% if _yn_schema.alternateName %}
			"alternateName": "{{_yn_schema.alternateName|raw}}",
		{% endif %}
		{% if _yn_schema.url %}
			"url": "{{_yn_schema.url|raw}}",
		{% endif %}
		{% if _yn_schema.keywords %}
			"keywords": "{{_yn_schema.keywords|raw}}",
		{% endif %}
		{% if _yn_schema.logo %}
			"logo": "{{_yn_schema.logo[0].path|raw}}",
		{% endif %}
		{% if _yn_schema.contact %}
			"contactPoint": [
				{% for _yn_contact in _yn_schema.contact %}
					{
						"@type": "ContactPoint",
						"telephone": "{{_yn_contact.phone|raw}}",
						"contactType": "{{_yn_contact.variant|raw}}",
						{% if _yn_contact.areas %}
							"areaServed": ["{% for _yn_lang in _yn_contact.areas %}{{_yn_lang|raw}}{% if not loop.last %},{% endif %}{% endfor %}"],
						{% endif %}
						{% if _yn_contact.languages %}
							"availableLanguage": ["{% for _yn_lang in _yn_contact.languages %}{{_yn_lang|raw}}{% if not loop.last %},{% endif %}{% endfor %}"],
						{% endif %}
					}{% if not loop.last %},{% endif %}
				{% endfor %}
			],
		{% endif %}
		{% if _yn_schema.sameAs %}
			"sameAs": [{% for _yn_sameAs in _yn_schema.sameAs %}"{{_yn_sameAs.url|raw}}"{% if not loop.last %},{% endif %}{% endfor %}]
		{% endif %}
		{% if _yn_schema.knowsAbout %}
            "knowsAbout": [
                {% for _yn_knowsAbout in _yn_schema.knowsAbout %}
                    "{{_yn_knowsAbout.text|raw}}"{% if not loop.last %},{% endif %}
                {% endfor %}
            ],
        {% endif %}
		{% if _yn_schema.awards %}
            "awards": [
                {% for _yn_awards in _yn_schema.awards %}
                    "{{_yn_awards.award|raw}}"{% if not loop.last %},{% endif %}
                {% endfor %}
            ],
        {% endif %}
	}
</script>