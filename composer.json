{
	"name": "ypsolution/ynfinite-php-client",
	"description": "PHP Client for Ynfinite",
	"type": "library",
	"license": "MIT",
	"homepage": "https://www.ynfinite.com",
	"keywords": [
		"ynfinite",
		"cms",
		"saas"
	],
	"authors": [
		{
			"name": "ypsolution GmbH",
			"email": "hello@ynfinite.com"
		}
	],
	"require": {
		"php": "^7.4 || ^8.0",
		"ext-json": "*",
		"bryanjhv/slim-session": "^4.1.2",
		"cocur/slugify": "^4.2.0",
		"curl/curl": "^2.3",
		"illuminate/database": "^9.32",
		"monolog/monolog": "1.25.3",
		"php-di/php-di": "^6.4.0",
		"slim/psr7": "^1.5.0",
		"slim/slim": "^4.10.0",
		"slim/twig-view": "^3.3.0",
		"twig/intl-extra": "3.8.0",
		"twig/twig": "3.8.0",
		"vlucas/phpdotenv": "^5.4.1",
		"whichbrowser/parser": "^2.0",
		"plesk/composer-command-info": "~1.1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"config": {
		"process-timeout": 0,
		"sort-packages": true,
		"allow-plugins": {
			"plesk/composer-command-info": true
		}
	}
}
